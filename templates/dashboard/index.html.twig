{% extends 'base_dashboard.html.twig' %}

{% block title %}Dashboard{% endblock %}

{% block body %}
    <div class="card">
        <div class="card-body">
            {% if is_granted('IS_AUTHENTICATED_FULLY') %}
                <h1 class="card-title">Welcome {{ app.user.username }}</h1>
            {% else %}
                <h1 class="card-title">Welcome Guest</h1>
            {% endif %}
        </div>
    </div>
{% endblock %}