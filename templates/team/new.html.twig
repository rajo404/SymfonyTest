{% extends 'base.html.twig' %}

{% block title %}Add New Team{% endblock %}

{% block body %}
    <h1>Add New Team</h1>

    {{ form_start(form) }}
    {{ form_row(form.name) }}
    {{ form_row(form.country) }}
    {{ form_row(form.balance) }}

    <button type="submit" class="btn btn-primary">Save</button>
    {{ form_end(form) }}
{% endblock %}
